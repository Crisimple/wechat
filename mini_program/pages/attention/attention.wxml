<view class="attention">
    <view class="attention_title">你的关注</view>
    <view class="attention_item" wx:for="{{ attention_ups }}" wx:for-index="up_id" wx:for-item="up_nickname">
        {{ up_id }} -- {{ up_nickname }}
    </view>

    <view>UP详细信息</view>
    <view class="up_detail" wx:for="{{up_detail}}">
        {{ up_detail }}
    </view>
</view>
